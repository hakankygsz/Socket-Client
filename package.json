    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tsc-alias": "^1.8.15",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.17.0",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/socket.io": "^3.0.1",
